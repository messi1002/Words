<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>a1</servlet-name>
        <url-pattern>/Servlet/Login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>a1</servlet-name>
        <servlet-class>Servlet.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>a2</servlet-name>
        <url-pattern>/Servlet/Register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>a2</servlet-name>
        <servlet-class>Servlet.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>a3</servlet-name>
        <url-pattern>/Servlet/Recite</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>a3</servlet-name>
        <servlet-class>Servlet.Recite</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>a4</servlet-name>
        <url-pattern>/Servlet/Review</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>a4</servlet-name>
        <servlet-class>Servlet.Review</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>a5</servlet-name>
        <url-pattern>/Servlet/Search</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>a5</servlet-name>
        <servlet-class>Servlet.Search</servlet-class>
    </servlet>
</web-app>